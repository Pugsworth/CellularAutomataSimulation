{
    "compilerOptions": {
        "target": "ES6",
        "baseUrl": ".",
        "moduleResolution": "Node",
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ],
            "@/*": ["src/*"],
            "lib/*": ["src/lib/*"]
        },
        "rootDirs": [
            "src",
            "src/lib"
        ]
    }
}